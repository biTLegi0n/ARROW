<h1>
  Profile: 
</h1>
<div class="col-xl-3 col-sm-6 col-12">
    <div class="card">
      <div class="card-content">
        <div class="card-body">
          <div class="media d-flex">
            <div class="media-body text-left">
              <h3 class="success">
                {{profile.displayName}}
              </h3>
              <span class="up">
                {{profile.role}}
              </span>
            </div>
            <div class="align-self-center">
                <img src={{profile.image}} class="float-right" />
              </div>
          </div>
          <div class="progress mt-1 mb-0" style="height: 7px;">
            <div
              class="progress-bar bg-success"
              role="progressbar"
              style="width: 100%"
              aria-valuenow="50"
              aria-valuemin="0"
              aria-valuemax="100"
            ></div>
          </div>
          <div class="media-body mt-1 mb-0 text-left" onclick="on()">
            <span>
              {{profile.email}}
            </span>
          </div>
        </div>
      </div>
    </div>
  </div>

<h1>
  Prescriptions:
</h1>
{{! <div class="flex"> }}
  <div>
{{#each prescriptions}}
  <div class="flex pres">
    <a href="/result/{{this.resultId}}" class="col-xl-3 col-sm-6 col-12">
    <div class="card">
      <div class="card-content">
        <div class="card-body">
          <div class="media d-flex">
            <div class="media-body text-left">
              <h3 class="success">
                {{this.predictionMain.[1]}}
              </h3>
              <span>
                {{this.predictionMain.[0]}}
              </span>
            </div>
          </div>
          <div class="progress mt-1 mb-0" style="height: 7px;">
            <div
              class="progress-bar bg-success"
              role="progressbar"
              style="width: {{this.predictionMain.[1]}}"
              aria-valuenow="50"
              aria-valuemin="0"
              aria-valuemax="100"
            ></div>
          </div>
          <div class="media-body mt-1 mb-0 text-left" onclick="on()">
            <span>
              {{createdAt}}
            </span>
          </div>
        </div>
      </div>
    </div>
  </a>
    {{!-- <textarea name="prescription">{{this.prescription}}</textarea> --}}
    {{! <div class="w-50"> }}
    {{! <div class="card"> }}
    <div class="ui form">
      <div class="field">
        <textarea name="prescription" disabled>{{this.prescription}}</textarea>
      </div>
    </div>
    <div>
      <span>{{this.createdAt}}</span>
    </div>
    {{!-- <div class="card-content">
          <div class="card-body">
            <div class="media d-flex">
              <div class="media-body text-left">
                <h3>
                  {{this.prescription}}
                </h3>
              </div>
            </div>
          </div>
        </div> --}}
    {{! </div> }}
    {{! </div> }}
  </div>
{{/each}}
</div>
{{! </div> }}