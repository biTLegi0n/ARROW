<h1>
    {{role}} :
</h1>
<div class="flex">
    {{#each users}}
    <a href="/user/{{this.googleId}}" class="col-xl-3 col-sm-6 col-12">
        <div class="card">
            <div class="card-content">
                <div class="card-body">
                    <div class="media d-flex">
                        <div class="media-body text-left">
                            <h3 class="success">
                                {{this.displayName}}
                            </h3>
                            <span>
                                {{this.email}}
                            </span>
                        </div>
                        <div class="align-self-center">
                            <img src={{this.image}} class="float-right" />
                        </div>
                    </div>
                    <div class="progress mt-1 mb-0" style="height: 7px;">
                        <div class="progress-bar bg-success" role="progressbar" style="width: 100%" aria-valuenow="50"
                            aria-valuemin="0" aria-valuemax="100"></div>
                    </div>
                    <div class="media-body mt-1 mb-0 text-left" onclick="on()">
                        <span>
                            {{this.createdAt}}
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </a>
    {{/each}}
</div>